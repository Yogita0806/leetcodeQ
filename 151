for(int i=n-1;i>=0;i--)
        {
            if(s.charAt(i)==' ')
            {
                news=news+s.charAt(i);
            }
            else
            {
                int start=i;
                int end=i;
                while(s.charAt(i)!=' ')
                {
                    i--;
                    start--;
                }
                news=news + s.substring(start,end+1);
               
            }
        }





