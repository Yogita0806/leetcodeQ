class Solution
{
    
    
    static void reverse(Stack<Integer> s)
    {
        add your code here
        if(s.isEmpty()){
            return;
        }
        while(!s.isEmpty()){
            s1.push(s.pop());
        }
        while(!s1.isEmpty()){
            s2.push(s1.pop());
        }
        while(!s2.isEmpty()){
            s.push(s2.pop());
        }
        
        
    }
}
